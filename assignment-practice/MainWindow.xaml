<Window x:Class="assignment_practice.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:assignment_practice"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="455.805">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Label Content="Support Ticket System" Grid.Row="0" Margin="74,34,37,26" FontSize="30"/>

        <TextBox Name="Username" Grid.Row="1" Margin="30,0,30,60"/>
        <TextBox Name="Passwords" Grid.Row="1" Margin="30,60,30,0"/>

        <Button x:Name="button1" Click="UserLogin" Content="Login as User" Grid.Row="2"  Margin="61,30,287,30" Width="100" HorizontalAlignment="Center"/>
        <Button x:Name="button2" Content="Login as Admin" Click="AdminLogin" Grid.Row="2"  Margin="273,30,75,30" Width="100" HorizontalAlignment="Center"/>


    </Grid>
</Window>
